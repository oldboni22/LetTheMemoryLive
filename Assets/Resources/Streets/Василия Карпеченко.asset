  _name: "\u0412\u0430\u0441\u0438\u043B\u0438\u044F \u041A\u0430\u0440\u043F\u0435\u0447\u0435\u043D\u043A\u043E"
  _subjectName: "\u041A\u0430\u0440\u043F\u0435\u0447\u0435\u043D\u043A\u043E \u0412\u0430\u0441\u0438\u043B\u0438\u0439
    \u0412\u0430\u0441\u0438\u043B\u044C\u0435\u0432\u0438\u0447"
  _info: "\u0421\u043E\u0432\u0435\u0442\u0441\u043A\u0438\u0439 \u0432\u043E\u0435\u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A,
    \u043F\u043E\u043B\u043A\u043E\u0432\u043D\u0438\u043A. \u0423\u0447\u0430\u0441\u0442\u043D\u0438\u043A
    \u043E\u0431\u043E\u0440\u043E\u043D\u044B \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0430
    \u043E\u0442 \u043D\u0435\u043C\u0435\u0446\u043A\u043E-\u0444\u0430\u0448\u0438\u0441\u0442\u0441\u043A\u0438\u0445
    \u0437\u0430\u0445\u0432\u0430\u0442\u0447\u0438\u043A\u043E\u0432 \u0432 \u0433\u043E\u0434\u044B
    \u0412\u0435\u043B\u0438\u043A\u043E\u0439 \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B. \u0420\u043E\u0434\u0438\u043B\u0441\u044F \u0432
    \u0434\u0435\u0440\u0435\u0432\u043D\u0435 \u0411\u043E\u0440\u043E\u0434\u0438\u043D\u043E
    (\u043D\u044B\u043D\u0435 \u0432 \u0412\u044B\u0433\u043E\u043D\u0438\u0447\u0441\u043A\u0438\u0439
    \u0440\u0430\u0439\u043E\u043D, \u0411\u0440\u044F\u043D\u0441\u043A\u0430\u044F
    \u043E\u0431\u043B\u0430\u0441\u0442\u044C). \u041E\u043A\u043E\u043D\u0447\u0438\u043B
    \u0441\u0435\u043B\u044C\u0441\u043A\u0443\u044E \u0448\u043A\u043E\u043B\u0443,
    \u0447\u0435\u0442\u044B\u0440\u0435 \u043A\u043B\u0430\u0441\u0441\u0430 \u0411\u0440\u044F\u043D\u0441\u043A\u043E\u0439
    \u043C\u0443\u0436\u0441\u043A\u043E\u0439 \u0433\u0438\u043C\u043D\u0430\u0437\u0438\u0438.
    15 \u043C\u0430\u044F 1916 \u0433. \u0431\u044B\u043B \u0432\u043F\u0435\u0440\u0432\u044B\u0435
    \u043C\u043E\u0431\u0438\u043B\u0438\u0437\u043E\u0432\u0430\u043D \u043D\u0430
    \u0432\u043E\u0435\u043D\u043D\u0443\u044E \u0441\u043B\u0443\u0436\u0431\u0443.
    \u0421 \u0430\u0432\u0433\u0443\u0441\u0442\u0430 1919 \u0433. \u0443\u0447\u0438\u043B\u0441\u044F
    \u043D\u0430 \u041C\u0438\u043D\u0441\u043A\u0438\u0445 \u043A\u043E\u043C\u0430\u043D\u0434\u043D\u044B\u0445
    \u043A\u0443\u0440\u0441\u0430\u0445 \u0432 \u0433. \u0413\u043E\u043C\u0435\u043B\u044C.
    \u0412 \u0430\u0432\u0433\u0443\u0441\u0442\u0435 1920 \u0433. \u043E\u043A\u043E\u043D\u0447\u0438\u043B
    \u0438\u0445 \u0432 \u0433. \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0435.
    \u0412 \u0438\u044E\u043B\u0435 1940 \u0433. \u0432\u0441\u0442\u0443\u043F\u0438\u043B
    \u0432 \u0434\u043E\u043B\u0436\u043D\u043E\u0441\u0442\u044C \u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A\u0430
    1-\u0433\u043E (\u043E\u043F\u0435\u0440\u0430\u0442\u0438\u0432\u043D\u043E\u0433\u043E)
    \u043E\u0442\u0434\u0435\u043B\u0435\u043D\u0438\u044F \u0448\u0442\u0430\u0431\u0430
    172-\u0439 \u0441\u0442\u0440\u0435\u043B\u043A\u043E\u0432\u043E\u0439 \u0434\u0438\u0432\u0438\u0437\u0438\u0438
    \u041C\u0412\u041E \u0432 \u0433. \u0421\u0442\u0430\u043B\u0438\u043D\u043E\u0433\u043E\u0440\u0441\u043A
    \u0422\u0443\u043B\u044C\u0441\u043A\u043E\u0439 \u043E\u0431\u043B\u0430\u0441\u0442\u0438.
    \u0412 \u043A\u043E\u043D\u0446\u0435 \u0438\u044E\u043D\u044F \u2014 \u043D\u0430\u0447\u0430\u043B\u0435
    \u0438\u044E\u043B\u044F 1941 \u0433. \u0432 \u0441\u043E\u0441\u0442\u0430\u0432\u0435
    172-\u0439 \u0441\u0442\u0440\u0435\u043B\u043A\u043E\u0432\u043E\u0439 \u0434\u0438\u0432\u0438\u0437\u0438\u0438
    \u043F\u0440\u0438\u043D\u0438\u043C\u0430\u043B \u0430\u043A\u0442\u0438\u0432\u043D\u043E\u0435
    \u0443\u0447\u0430\u0441\u0442\u0438\u0435 \u0432 \u043E\u0431\u043E\u0440\u043E\u043D\u0435
    \u0433. \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0430. \u0417\u0430 \u0432\u044B\u043F\u043E\u043B\u043D\u0435\u043D\u0438\u0435
    \u043F\u043E\u0441\u0442\u0430\u0432\u043B\u0435\u043D\u043D\u044B\u0445 \u0437\u0430\u0434\u0430\u0447
    \u0423\u043A\u0430\u0437\u043E\u043C \u041F\u0440\u0435\u0437\u0438\u0434\u0438\u0443\u043C\u0430
    \u0412\u0435\u0440\u0445\u043E\u0432\u043D\u043E\u0433\u043E \u0421\u043E\u0432\u0435\u0442\u0430
    \u0421\u0421\u0421\u0420 \u043E\u0442 9 \u0430\u0432\u0433\u0443\u0441\u0442\u0430
    1941 \u0433. \u0412.\u0410. \u041A\u0430\u0442\u044E\u0448\u0438\u043D \u0431\u044B\u043B
    \u043D\u0430\u0433\u0440\u0430\u0436\u0434\u0435\u043D \u043E\u0440\u0434\u0435\u043D\u043E\u043C
    \u041A\u0440\u0430\u0441\u043D\u043E\u0439 \u0417\u0432\u0435\u0437\u0434\u044B.
    \u0417\u0430 \u0432\u0440\u0435\u043C\u044F \u0432\u043E\u0439\u043D\u044B \u043A\u043E\u043C\u0434\u0438\u0432
    \u041A\u0430\u0442\u044E\u0448\u0438\u043D \u0431\u044B\u043B \u043F\u044F\u0442\u044C
    \u0440\u0430\u0437 \u043F\u0435\u0440\u0441\u043E\u043D\u0430\u043B\u044C\u043D\u043E
    \u0443\u043F\u043E\u043C\u044F\u043D\u0443\u0442 \u0432 \u0431\u043B\u0430\u0433\u043E\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0445
    \u043F\u0440\u0438\u043A\u0430\u0437\u0430\u0445 \u0412\u0435\u0440\u0445\u043E\u0432\u043D\u043E\u0433\u043E
    \u0413\u043B\u0430\u0432\u043D\u043E\u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0435\u0433\u043E.
    \u0421 \u0438\u044E\u043D\u044F 1947 \u0433. \u0438\u0441\u043F\u043E\u043B\u043D\u044F\u043B
    \u0434\u043E\u043B\u0436\u043D\u043E\u0441\u0442\u044C \u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A\u0430
    \u0432\u043E\u0435\u043D\u043D\u043E\u0439 \u043A\u0430\u0444\u0435\u0434\u0440\u044B
    \u0411\u0435\u043B\u043E\u0440\u0443\u0441\u0441\u043A\u043E\u0433\u043E \u0441\u0435\u043B\u044C\u0441\u043A\u043E\u0445\u043E\u0437\u044F\u0439\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0433\u043E
    \u0438\u043D\u0441\u0442\u0438\u0442\u0443\u0442\u0430, \u043F\u0440\u0435\u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u043D\u043D\u043E\u0433\u043E
    \u0437\u0430\u0442\u0435\u043C \u0432 \u0441\u0435\u043B\u044C\u0441\u043A\u043E\u0445\u043E\u0437\u044F\u0439\u0441\u0442\u0432\u0435\u043D\u043D\u0443\u044E
    \u0430\u043A\u0430\u0434\u0435\u043C\u0438\u044E (\u0433. \u0413\u043E\u0440\u043A\u0438).
    11 \u0430\u0432\u0433\u0443\u0441\u0442\u0430 1953 \u0433. \u043F\u043E\u043B\u043A\u043E\u0432\u043D\u0438\u043A
    \u0412.\u0410. \u041A\u0430\u0442\u044E\u0448\u0438\u043D \u0443\u0432\u043E\u043B\u0438\u043B\u0441\u044F
    \u0432 \u0437\u0430\u043F\u0430\u0441 \u0438 \u0443\u0435\u0445\u0430\u043B \u0432
    \u0433. \u0428\u043E\u0441\u0442\u043A\u0430 \u0421\u0443\u043C\u0441\u043A\u043E\u0439
    \u043E\u0431\u043B\u0430\u0441\u0442\u0438, \u0433\u0434\u0435 \u0436\u0438\u043B
    \u0435\u0433\u043E \u0441\u044B\u043D, \u0433\u043E\u0434 \u0441\u043F\u0443\u0441\u0442\u044F
    \u0443\u0448\u0451\u043B \u0438\u0437 \u0436\u0438\u0437\u043D\u0438 \u2013 23
    \u043D\u043E\u044F\u0431\u0440\u044F 1954 \u0433. \u041D\u0430\u0433\u0440\u0430\u0434\u044B:
    \u043E\u0440\u0434\u0435\u043D \u041B\u0435\u043D\u0438\u043D\u0430 (1945); \u0434\u0432\u0430
    \u043E\u0440\u0434\u0435\u043D\u0430 \u041A\u0440\u0430\u0441\u043D\u043E\u0433\u043E
    \u0417\u043D\u0430\u043C\u0435\u043D\u0438 (1943, 1944 \u0438 1948); \u043E\u0440\u0434\u0435\u043D
    \u0421\u0443\u0432\u043E\u0440\u043E\u0432\u0430 II \u0441\u0442\u0435\u043F\u0435\u043D\u0438
    (1945); \u043E\u0440\u0434\u0435\u043D \u041A\u0440\u0430\u0441\u043D\u043E\u0439
    \u0417\u0432\u0435\u0437\u0434\u044B (1941). \u041C\u0435\u0434\u0430\u043B\u0438:
    \"XX \u043B\u0435\u0442 \u0420\u0430\u0431\u043E\u0447\u0435-\u041A\u0440\u0435\u0441\u0442\u044C\u044F\u043D\u0441\u043A\u043E\u0439
    \u041A\u0440\u0430\u0441\u043D\u043E\u0439 \u0410\u0440\u043C\u0438\u0438\";
    \"\u0417\u0430 \u043E\u0431\u043E\u0440\u043E\u043D\u0443 \u041C\u043E\u0441\u043A\u0432\u044B\"
    (1944); \"\u0417\u0430 \u043E\u0431\u043E\u0440\u043E\u043D\u0443 \u0421\u0442\u0430\u043B\u0438\u043D\u0433\u0440\u0430\u0434\u0430\";
    \"\u0417\u0430 \u043F\u043E\u0431\u0435\u0434\u0443 \u043D\u0430\u0434 \u0413\u0435\u0440\u043C\u0430\u043D\u0438\u0435\u0439
    \u0432 \u0412\u0435\u043B\u0438\u043A\u043E\u0439 \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0432\u043E\u0439\u043D\u0435 1941\u20141945 \u0433\u0433.\" (1945); \"\u0417\u0430
    \u043E\u0441\u0432\u043E\u0431\u043E\u0436\u0434\u0435\u043D\u0438\u0435 \u041F\u0440\u0430\u0433\u0438\"."
