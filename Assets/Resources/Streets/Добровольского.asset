  _name: "\u0414\u043E\u0431\u0440\u043E\u0432\u043E\u043B\u044C\u0441\u043A\u043E\u0433\u043E"
  _subjectName: "\u0414\u043E\u0431\u0440\u043E\u0432\u043E\u043B\u044C\u0441\u043A\u0438\u0439
    \u0415\u0440\u043E\u0444\u0435\u0439 \u0412\u043B\u0430\u0434\u0438\u043C\u0438\u0440\u043E\u0432\u0438\u0447"
  _info: "\u0413\u0435\u0440\u043E\u0439 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430 (06.04.1945), \u0433\u0435\u043D\u0435\u0440\u0430\u043B-\u043B\u0435\u0439\u0442\u0435\u043D\u0430\u043D\u0442
    (1945). \u0420\u043E\u0434\u0438\u043B\u0441\u044F 09.11.1903 \u0433. \u0432
    \u0411\u0435\u043B\u0430\u0440\u0443\u0441\u0438, \u0432 \u0434\u0435\u0440\u0435\u0432\u043D\u0435
    \u0428\u0430\u043B\u0430\u0451\u0432\u043A\u0430 \u041A\u0438\u0440\u043E\u0432\u0441\u043A\u043E\u0433\u043E
    \u0440\u0430\u0439\u043E\u043D\u0430 \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0441\u043A\u043E\u0439
    \u043E\u0431\u043B\u0430\u0441\u0442\u0438 \u0432 \u043A\u0440\u0435\u0441\u0442\u044C\u044F\u043D\u0441\u043A\u043E\u0439
    \u0441\u0435\u043C\u044C\u0435. \u0412 \u041A\u0440\u0430\u0441\u043D\u043E\u0439
    \u0410\u0440\u043C\u0438\u0438 \u0441 1925 \u0433. \u041E\u043A\u043E\u043D\u0447\u0438\u043B
    \u041A\u0438\u0435\u0432\u0441\u043A\u043E\u0435 \u0432\u043E\u0435\u043D\u043D\u043E\u0435
    \u043F\u0435\u0445\u043E\u0442\u043D\u043E\u0435 \u0443\u0447\u0438\u043B\u0438\u0449\u0435
    (1931), \u043A\u0443\u0440\u0441\u044B \"\u0412\u044B\u0441\u0442\u0440\u0435\u043B\"
    (1940). \u041A\u043E\u043C\u0430\u043D\u0434\u043E\u0432\u0430\u043B \u0432\u0437\u0432\u043E\u0434\u043E\u043C,
    \u0440\u043E\u0442\u043E\u0439, \u0431\u0430\u0442\u0430\u043B\u044C\u043E\u043D\u043E\u043C,
    \u043F\u043E\u043B\u043A\u043E\u043C. \u0412 \u0412\u0435\u043B\u0438\u043A\u0443\u044E
    \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u0443\u044E
    \u0432\u043E\u0439\u043D\u0443 \u0441 \u0438\u044E\u043B\u044F 1941 \u0433. \u043D\u0430
    \u0417\u0430\u043F\u0430\u0434\u043D\u043E\u043C, \u041A\u0430\u043B\u0438\u043D\u0438\u043D\u0441\u043A\u043E\u043C,
    1-\u043C \u041F\u0440\u0438\u0431\u0430\u043B\u0442\u0438\u0439\u0441\u043A\u043E\u043C
    \u0438 1-\u043C \u0411\u0435\u043B\u043E\u0440\u0443\u0441\u0441\u043A\u043E\u043C
    \u0444\u0440\u043E\u043D\u0442\u0430\u0445: \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440
    \u043F\u043E\u043B\u043A\u0430, \u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A
    \u0448\u0442\u0430\u0431\u0430, \u0437\u0430\u043C\u0435\u0441\u0442\u0438\u0442\u0435\u043B\u044C
    \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440\u0430, \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440
    \u0441\u0442\u0440\u0435\u043B\u043A\u043E\u0432\u043E\u0439 \u0434\u0438\u0432\u0438\u0437\u0438\u0438,
    \u0441 \u043E\u043A\u0442\u044F\u0431\u0440\u044F 1943 \u0433. \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440
    \u0441\u0442\u0440\u0435\u043B\u043A\u043E\u0432\u043E\u0433\u043E \u043A\u043E\u0440\u043F\u0443\u0441\u0430.
    \u041A\u043E\u043C\u0430\u043D\u0434\u0443\u044F 16-\u043C \u0441\u0442\u0440\u0435\u043B\u043A\u043E\u0432\u044B\u043C
    \u043A\u043E\u0440\u043F\u0443\u0441\u043E\u043C (33-\u044F \u0430\u0440\u043C\u0438\u044F,
    1-\u0439 \u0411\u0435\u043B\u043E\u0440\u0443\u0441\u0441\u043A\u0438\u0439 \u0444\u0440\u043E\u043D\u0442),
    \u0433\u0432\u0430\u0440\u0434\u0438\u0438 \u0433\u0435\u043D\u0435\u0440\u0430\u043B-\u043C\u0430\u0439\u043E\u0440
    \u0415.\u0412. \u0414\u043E\u0431\u0440\u043E\u0432\u043E\u043B\u044C\u0441\u043A\u0438\u0439
    \u043E\u0442\u043B\u0438\u0447\u0438\u043B\u0441\u044F \u0432 \u044F\u043D\u0432\u0430\u0440\u0435
    1945 \u0433. \u0432 \u0412\u0438\u0441\u043B\u043E-\u041E\u0434\u0435\u0440\u0441\u043A\u043E\u0439
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u0438. \u0412\u043E\u0439\u0441\u043A\u0430
    \u043A\u043E\u0440\u043F\u0443\u0441\u0430, \u043F\u0440\u0435\u0441\u043B\u0435\u0434\u0443\u044F
    \u043F\u0440\u043E\u0442\u0438\u0432\u043D\u0438\u043A\u0430, \u0441 \u0445\u043E\u0434\u0443
    \u0444\u043E\u0440\u0441\u0438\u0440\u043E\u0432\u0430\u043B\u0438 \u0440\u0435\u043A\u0443
    \u0412\u0430\u0440\u0442\u0430 \u0432 \u0440\u0430\u0439\u043E\u043D\u0435 \u0433\u043E\u0440\u043E\u0434\u0430
    \u041A\u0430\u0437\u0438\u043C\u0435\u0436 (\u043D\u044B\u043D\u0435 \u041A\u0430\u0437\u0438\u043C\u0435\u0436-\u0414\u043E\u043B\u044C\u043D\u044B,
    \u041F\u043E\u043B\u044C\u0448\u0430), 20.01.1945 \u0433. \u043E\u0432\u043B\u0430\u0434\u0435\u043B\u0438
    \u0433\u043E\u0440\u043E\u0434\u043E\u043C \u041A\u0430\u043B\u0438\u0448 (\u041F\u043E\u043B\u044C\u0448\u0430).
    \u0412 \u044D\u0442\u0438\u0445 \u0431\u043E\u044F\u0445 \u043A\u043E\u0440\u043F\u0443\u0441\u043E\u043C
    \u0443\u043D\u0438\u0447\u0442\u043E\u0436\u0435\u043D\u043E 549 \u0432\u0440\u0430\u0436\u0435\u0441\u043A\u0438\u0445
    \u043E\u0440\u0443\u0434\u0438\u0439, 123 \u0442\u0430\u043D\u043A\u0430, 100
    \u0441\u0430\u043C\u043E\u0445\u043E\u0434\u043D\u044B\u0445 \u043E\u0440\u0443\u0434\u0438\u0439,
    4011 \u0430\u0432\u0442\u043E\u043C\u0430\u0448\u0438\u043D, 200 \u043C\u043E\u0442\u043E\u0446\u0438\u043A\u043B\u043E\u0432,
    \u043E\u043A\u043E\u043B\u043E 7160 \u0432\u0440\u0430\u0436\u0435\u0441\u043A\u0438\u0445
    \u0441\u043E\u043B\u0434\u0430\u0442 \u0438 \u043E\u0444\u0438\u0446\u0435\u0440\u043E\u0432,
    1270 \u0432\u0437\u044F\u0442\u043E \u0432 \u043F\u043B\u0435\u043D. \u041D\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043D
    2 \u043E\u0440\u0434\u0435\u043D\u0430\u043C\u0438 \u041B\u0435\u043D\u0438\u043D\u0430,
    4 \u043E\u0440\u0434\u0435\u043D\u0430\u043C\u0438 \u041A\u0440\u0430\u0441\u043D\u043E\u0433\u043E
    \u0417\u043D\u0430\u043C\u0435\u043D\u0438, 2 \u043E\u0440\u0434\u0435\u043D\u0430\u043C\u0438
    \u0421\u0443\u0432\u043E\u0440\u043E\u0432\u0430 II \u0441\u0442\u0435\u043F\u0435\u043D\u0438,
    \u043E\u0440\u0434\u0435\u043D\u0430\u043C\u0438 \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B I \u0441\u0442\u0435\u043F\u0435\u043D\u0438,
    \u041A\u0440\u0430\u0441\u043D\u043E\u0439 \u0417\u0432\u0435\u0437\u0434\u044B,
    \"\u0417\u043D\u0430\u043A \u041F\u043E\u0447\u0451\u0442\u0430\", \u043C\u0435\u0434\u0430\u043B\u044F\u043C\u0438.
    \u041F\u043E\u0441\u043B\u0435 \u0432\u043E\u0439\u043D\u044B \u043E\u043A\u043E\u043D\u0447\u0438\u043B
    \u0412\u044B\u0441\u0448\u0438\u0435 \u0430\u043A\u0430\u0434\u0435\u043C\u0438\u0447\u0435\u0441\u043A\u0438\u0435
    \u043A\u0443\u0440\u0441\u044B \u043F\u0440\u0438 \u0412\u043E\u0435\u043D\u043D\u043E\u0439
    \u0430\u043A\u0430\u0434\u0435\u043C\u0438\u0438 \u0413\u0435\u043D\u0448\u0442\u0430\u0431\u0430
    (1947). \u0414\u043E 1958 \u0433. \u0432 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0439
    \u0410\u0440\u043C\u0438\u0438. \u041A\u043E\u043C\u0430\u043D\u0434\u043E\u0432\u0430\u043B
    \u0432\u043E\u0437\u0434\u0443\u0448\u043D\u043E-\u0434\u0435\u0441\u0430\u043D\u0442\u043D\u044B\u043C
    \u0441\u043E\u0435\u0434\u0438\u043D\u0435\u043D\u0438\u0435\u043C, \u0431\u044B\u043B
    \u043F\u043E\u043C\u043E\u0449\u043D\u0438\u043A\u043E\u043C \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0435\u0433\u043E
    \u0432\u043E\u0437\u0434\u0443\u0448\u043D\u043E-\u0434\u0435\u0441\u0430\u043D\u0442\u043D\u044B\u043C\u0438
    \u0432\u043E\u0439\u0441\u043A\u0430\u043C\u0438. \u0416\u0438\u043B \u0432 \u041C\u043E\u0441\u043A\u0432\u0435.
    \u0420\u0430\u0431\u043E\u0442\u0430\u043B \u0432 \u041C\u0438\u043D\u0438\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0435
    \u0433\u0430\u0437\u043E\u0432\u043E\u0439 \u043F\u0440\u043E\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u043E\u0441\u0442\u0438
    \u0421\u0421\u0421\u0420. \u0423\u043C\u0435\u0440 21.04.1987 \u0433."
