  _name: "\u041A\u043E\u0436\u0435\u0434\u0443\u0431\u0430"
  _subjectName: "\u041A\u043E\u0436\u0435\u0434\u0443\u0431 \u0418\u0432\u0430\u043D
    \u041D\u0438\u043A\u0438\u0442\u043E\u0432\u0438\u0447"
  _info: "\u0421\u043E\u0432\u0435\u0442\u0441\u043A\u0438\u0439 \u0432\u043E\u0435\u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A,
    \u043C\u0430\u0440\u0448\u0430\u043B \u0430\u0432\u0438\u0430\u0446\u0438\u0438
    (1985), \u0442\u0440\u0438\u0436\u0434\u044B \u0413\u0435\u0440\u043E\u0439 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430 (04.02.1944, 19.08.1944, 18.08.1945). \u0420\u043E\u0434\u0438\u043B\u0441\u044F
    \u0432 \u0441. \u041E\u0431\u0440\u0430\u0436\u0438\u0435\u0432\u043A\u0430 \u0428\u043E\u0441\u0442\u043A\u0438\u043D\u0441\u043A\u043E\u0433\u043E
    \u0440\u0430\u0439\u043E\u043D\u0430 \u0421\u0443\u043C\u0441\u043A\u043E\u0439
    \u043E\u0431\u043B\u0430\u0441\u0442\u0438 (\u0423\u043A\u0440\u0430\u0438\u043D\u0430).
    \u0427\u043B\u0435\u043D \u041A\u041F\u0421\u0421 \u0441 1943 \u0433. \u0412
    \u041A\u0440\u0430\u0441\u043D\u043E\u0439 \u0410\u0440\u043C\u0438\u0438 \u0441
    1940 \u0433. \u0417\u0430\u043D\u0438\u043C\u0430\u043B\u0441\u044F \u0432 \u0428\u043E\u0441\u0442\u043A\u0438\u043D\u0441\u043A\u043E\u043C
    \u0430\u044D\u0440\u043E\u043A\u043B\u0443\u0431\u0435. \u041E\u043A\u043E\u043D\u0447\u0438\u043B
    \u0445\u0438\u043C\u0438\u043A\u043E-\u0442\u0435\u0445\u043D\u043E\u043B\u043E\u0433\u0438\u0447\u0435\u0441\u043A\u0438\u0439
    \u0442\u0435\u0445\u043D\u0438\u043A\u0443\u043C (1940), \u0427\u0443\u0433\u0443\u0435\u0432\u0441\u043A\u043E\u0435
    \u0432\u043E\u0435\u043D\u043D\u043E\u0435 \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0435
    \u0443\u0447\u0438\u043B\u0438\u0449\u0435 \u043B\u0451\u0442\u0447\u0438\u043A\u043E\u0432
    (1941), \u0412\u043E\u0435\u043D\u043D\u043E-\u0432\u043E\u0437\u0434\u0443\u0448\u043D\u0443\u044E
    \u0430\u043A\u0430\u0434\u0435\u043C\u0438\u044E (1949), \u0412\u043E\u0435\u043D\u043D\u0443\u044E
    \u0430\u043A\u0430\u0434\u0435\u043C\u0438\u044E \u0413\u0435\u043D\u0448\u0442\u0430\u0431\u0430
    (1956).  \u0412\u043E \u0432\u0440\u0435\u043C\u044F \u0412\u0435\u043B\u0438\u043A\u043E\u0439
    \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B \u043B\u0451\u0442\u0447\u0438\u043A-\u0438\u043D\u0441\u0442\u0440\u0443\u043A\u0442\u043E\u0440
    \u0432 \u0427\u0443\u0433\u0443\u0435\u0432\u0441\u043A\u043E\u043C \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u043C
    \u0443\u0447\u0438\u043B\u0438\u0449\u0435 (1941\u201342); \u0441 \u043C\u0430\u0440\u0442\u0430
    1943 \u0433. \u043D\u0430 \u0412\u043E\u0440\u043E\u043D\u0435\u0436\u0441\u043A\u043E\u043C,
    \u0421\u0442\u0435\u043F\u043D\u043E\u043C, 2-\u043C \u0423\u043A\u0440\u0430\u0438\u043D\u0441\u043A\u043E\u043C
    \u0438 1-\u043C \u0411\u0435\u043B\u043E\u0440\u0443\u0441\u0441\u043A\u043E\u043C
    \u0444\u0440\u043E\u043D\u0442\u0430\u0445: \u043B\u0451\u0442\u0447\u0438\u043A,
    \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440 \u0437\u0432\u0435\u043D\u0430,
    \u044D\u0441\u043A\u0430\u0434\u0440\u0438\u043B\u044C\u0438, \u0437\u0430\u043C.
    \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440\u0430 \u0438\u0441\u0442\u0440\u0435\u0431\u0438\u0442\u0435\u043B\u044C\u043D\u043E\u0433\u043E
    \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0433\u043E \u043F\u043E\u043B\u043A\u0430. 
    \u0417\u0430 20 \u0441\u0431\u0438\u0442\u044B\u0445 \u0441\u0430\u043C\u043E\u043B\u0451\u0442\u043E\u0432
    \u043F\u0440\u043E\u0442\u0438\u0432\u043D\u0438\u043A\u0430 \u043F\u0440\u0438\u0441\u0432\u043E\u0435\u043D\u043E
    \u0437\u0432\u0430\u043D\u0438\u0435 \u0413\u0435\u0440\u043E\u044F \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430, \u0437\u0430 34 \u043B\u0438\u0447\u043D\u043E
    \u0441\u0431\u0438\u0442\u044B\u0445 \u0441\u0430\u043C\u043E\u043B\u0451\u0442\u0430
    \u043F\u0440\u043E\u0442\u0438\u0432\u043D\u0438\u043A\u0430 \u043D\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043D
    \u0432\u0442\u043E\u0440\u043E\u0439 \u043C\u0435\u0434\u0430\u043B\u044C\u044E
    \"\u0417\u043E\u043B\u043E\u0442\u0430\u044F \u0417\u0432\u0435\u0437\u0434\u0430\".
    \u0412\u0441\u0435\u0433\u043E \u0441\u043E\u0432\u0435\u0440\u0448\u0438\u043B
    330 \u0431\u043E\u0435\u0432\u044B\u0445 \u0432\u044B\u043B\u0435\u0442\u043E\u0432,
    \u0443\u0447\u0430\u0441\u0442\u0432\u043E\u0432\u0430\u043B \u0432 120 \u0432\u043E\u0437\u0434\u0443\u0448\u043D\u044B\u0445
    \u0431\u043E\u044F\u0445, \u0441\u0431\u0438\u043B 62 \u0441\u0430\u043C\u043E\u043B\u0451\u0442\u0430
    \u043F\u0440\u043E\u0442\u0438\u0432\u043D\u0438\u043A\u0430 (\u0432 \u0442.\u0447.
    1 \u0440\u0435\u0430\u043A\u0442\u0438\u0432\u043D\u044B\u0439), \u0437\u0430
    \u0447\u0442\u043E \u043D\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043D \u0442\u0440\u0435\u0442\u044C\u0435\u0439
    \u043C\u0435\u0434\u0430\u043B\u044C\u044E \"\u0417\u043E\u043B\u043E\u0442\u0430\u044F
    \u0417\u0432\u0435\u0437\u0434\u0430\". \u0420\u0430\u0437\u0440\u0430\u0431\u043E\u0442\u0430\u043B
    \u0440\u044F\u0434 \u043D\u043E\u0432\u044B\u0445 \u043F\u0440\u0438\u0451\u043C\u043E\u0432
    \u0438 \u0441\u043F\u043E\u0441\u043E\u0431\u043E\u0432 \u0432\u0435\u0434\u0435\u043D\u0438\u044F
    \u0432\u043E\u0437\u0434\u0443\u0448\u043D\u043E\u0433\u043E \u0431\u043E\u044F. 
    \u041F\u043E\u0441\u043B\u0435 \u0432\u043E\u0439\u043D\u044B \u0437\u0430\u043C.
    \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440\u0430, \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440
    \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0439 \u0434\u0438\u0432\u0438\u0437\u0438\u0438,
    \u0437\u0430\u0442\u0435\u043C \u043D\u0430 \u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0445
    \u0434\u043E\u043B\u0436\u043D\u043E\u0441\u0442\u044F\u0445 \u0432 \u0412\u0412\u0421.
    \u0421 1964 \u0433. \u043F\u0435\u0440\u0432\u044B\u0439 \u0437\u0430\u043C\u0435\u0441\u0442\u0438\u0442\u0435\u043B\u044C
    \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0435\u0433\u043E \u0430\u0432\u0438\u0430\u0446\u0438\u0435\u0439
    \u041C\u043E\u0441\u043A\u043E\u0432\u0441\u043A\u043E\u0433\u043E \u0432\u043E\u0435\u043D\u043D\u043E\u0433\u043E
    \u043E\u043A\u0440\u0443\u0433\u0430. \u041F\u043E \u0432\u043E\u0437\u0432\u0440\u0430\u0449\u0435\u043D\u0438\u0438
    \u043D\u0430 \u0440\u043E\u0434\u0438\u043D\u0443 \u041D.\u0418. \u041A\u043E\u0436\u0435\u0434\u0443\u0431
    \u043E\u043A\u043E\u043D\u0447\u0438\u043B \u0430\u043A\u0430\u0434\u0435\u043C\u0438\u044E
    \u0413\u0435\u043D\u0448\u0442\u0430\u0431\u0430, \u0437\u0430\u043D\u0438\u043C\u0430\u043B
    \u0440\u044F\u0434 \u0432\u044B\u0441\u043E\u043A\u0438\u0445 \u043A\u043E\u043C\u0430\u043D\u0434\u043D\u044B\u0445
    \u0434\u043E\u043B\u0436\u043D\u043E\u0441\u0442\u0435\u0439 \u0432 \u0412\u0412\u0421. 
    \u0421 1978 \u0432 \u0413\u0440\u0443\u043F\u043F\u0435 \u0433\u0435\u043D\u0435\u0440\u0430\u043B\u044C\u043D\u044B\u0445
    \u0438\u043D\u0441\u043F\u0435\u043A\u0442\u043E\u0440\u043E\u0432 \u041C\u0438\u043D\u0438\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0430
    \u043E\u0431\u043E\u0440\u043E\u043D\u044B \u0421\u0421\u0421\u0420. \u0414\u0435\u043F\u0443\u0442\u0430\u0442
    \u0412\u0435\u0440\u0445\u043E\u0432\u043D\u043E\u0433\u043E \u0421\u043E\u0432\u0435\u0442\u0430
    \u0421\u0421\u0421\u0420 \u0432 1946\u201362 \u0433. \u0421 1967 \u0433. \u043F\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043B\u044C
    \u0424\u0435\u0434\u0435\u0440\u0430\u0446\u0438\u0438 \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0433\u043E
    \u0441\u043F\u043E\u0440\u0442\u0430 \u0421\u0421\u0421\u0420, \u0432\u0438\u0446\u0435-\u043F\u0440\u0435\u0437\u0438\u0434\u0435\u043D\u0442
    \u041C\u0435\u0436\u0434\u0443\u043D\u0430\u0440\u043E\u0434\u043D\u043E\u0439
    \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0439 \u0444\u0435\u0434\u0435\u0440\u0430\u0446\u0438\u0438.
    \u0412 1985 \u0433. \u041D.\u0418. \u041A\u043E\u0436\u0435\u0434\u0443\u0431\u0443
    \u0431\u044B\u043B\u043E \u043F\u0440\u0438\u0441\u0432\u043E\u0435\u043D\u043E
    \u0437\u0432\u0430\u043D\u0438\u0435 \u043C\u0430\u0440\u0448\u0430\u043B\u0430
    \u0430\u0432\u0438\u0430\u0446\u0438\u0438. \u0421\u043A\u043E\u043D\u0447\u0430\u043B\u0441\u044F
    8 \u0430\u0432\u0433\u0443\u0441\u0442\u0430 1991 \u0433. \u041F\u043E\u0445\u043E\u0440\u043E\u043D\u0435\u043D
    \u043D\u0430 \u041D\u043E\u0432\u043E\u0434\u0435\u0432\u0438\u0447\u044C\u0435\u043C
    \u043A\u043B\u0430\u0434\u0431\u0438\u0449\u0435 \u0432 \u041C\u043E\u0441\u043A\u0432\u0435."
