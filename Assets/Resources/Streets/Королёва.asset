  _name: "\u041A\u043E\u0440\u043E\u043B\u0451\u0432\u0430"
  _subjectName: "\u041A\u043E\u0440\u043E\u043B\u0451\u0432 \u041D\u0438\u043A\u043E\u043B\u0430\u0439
    \u0424\u0438\u043B\u0438\u043F\u043F\u043E\u0432\u0438\u0447"
  _info: "\u0413\u0435\u0440\u043E\u0439 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430 (01.01.1944), \u0433\u0435\u043D\u0435\u0440\u0430\u043B-\u043C\u0430\u0439\u043E\u0440
    (1943), \u043E\u0434\u0438\u043D \u0438\u0437 \u043E\u0440\u0433\u0430\u043D\u0438\u0437\u0430\u0442\u043E\u0440\u043E\u0432
    \u0438 \u0440\u0443\u043A\u043E\u0432\u043E\u0434\u0438\u0442\u0435\u043B\u0435\u0439
    \u043F\u0430\u0440\u0442\u0438\u0437\u0430\u043D\u0441\u043A\u043E\u0433\u043E
    \u0434\u0432\u0438\u0436\u0435\u043D\u0438\u044F \u0432 \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0441\u043A\u043E\u0439
    \u043E\u0431\u043B\u0430\u0441\u0442\u0438 \u0432 \u0412\u0435\u043B\u0438\u043A\u0443\u044E
    \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u0443\u044E
    \u0432\u043E\u0439\u043D\u0443. \u0420\u043E\u0434\u0438\u043B\u0441\u044F 01.07.1906
    \u0433. \u0432 \u0434\u0435\u0440\u0435\u0432\u043D\u0435 \u0410\u043C\u0438\u043D\u043E\u0432\u0438\u0447\u0438
    \u041E\u0441\u0438\u043F\u043E\u0432\u0438\u0447\u0441\u043A\u043E\u0433\u043E
    \u0440\u0430\u0439\u043E\u043D\u0430 \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0441\u043A\u043E\u0439
    \u043E\u0431\u043B\u0430\u0441\u0442\u0438 \u0432 \u043A\u0440\u0435\u0441\u0442\u044C\u044F\u043D\u0441\u043A\u043E\u0439
    \u0441\u0435\u043C\u044C\u0435.  \u0421 1930 \u0433. \u0440\u0430\u0431\u043E\u0442\u0430\u043B
    \u043D\u0430 \u0414\u0443\u0431\u0440\u043E\u0432\u0435\u043D\u0441\u043A\u043E\u043C
    \u043B\u0435\u0441\u043E\u043F\u0438\u043B\u044C\u043D\u043E\u043C \u0437\u0430\u0432\u043E\u0434\u0435
    \"1 \u041C\u0430\u044F\", \u0437\u0430\u0442\u0435\u043C \u043F\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043B\u0435\u043C
    \u043A\u043E\u043B\u0445\u043E\u0437\u0430, \u043F\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043B\u0435\u043C
    \u0441\u0435\u043B\u044C\u0441\u043E\u0432\u0435\u0442\u0430. \u0421 1937 \u0433.
    \u0437\u0430\u043C\u0435\u0441\u0442\u0438\u0442\u0435\u043B\u044C \u043F\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043B\u044F,
    \u0437\u0430\u0442\u0435\u043C \u043F\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043B\u044C
    \u041E\u0441\u0438\u043F\u043E\u0432\u0438\u0447\u0441\u043A\u043E\u0433\u043E
    \u0440\u0430\u0439\u0438\u0441\u043F\u043E\u043B\u043A\u043E\u043C\u0430.  \u0412
    \u0412\u0435\u043B\u0438\u043A\u0443\u044E \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u0443\u044E
    \u0432\u043E\u0439\u043D\u0443 \u0441 03.07.1941 \u0433. \u0432 \u043F\u0430\u0440\u0442\u0438\u0437\u0430\u043D\u0430\u0445,
    \u043A\u043E\u043C\u0430\u043D\u0434\u043E\u0432\u0430\u043B \u0433\u0440\u0443\u043F\u043F\u043E\u0439.
    \u0421 \u043C\u0430\u0440\u0442\u0430 1942 \u0433. \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440
    210-\u0433\u043E \u043F\u0430\u0440\u0442\u0438\u0437\u0430\u043D\u0441\u043A\u043E\u0433\u043E
    \u043E\u0442\u0440\u044F\u0434\u0430, \u0441 \u044F\u043D\u0432\u0430\u0440\u044F
    1943 \u0433. \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440 1-\u0439 \u041E\u0441\u0438\u043F\u043E\u0432\u0438\u0447\u0441\u043A\u043E\u0439
    \u043F\u0430\u0440\u0442\u0438\u0437\u0430\u043D\u0441\u043A\u043E\u0439 \u0431\u0440\u0438\u0433\u0430\u0434\u044B,
    \u043F\u0440\u0435\u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u043D\u043D\u043E\u0439
    \u0432 \u0438\u044E\u043B\u0435 1943 \u0433. \u0432 \u041E\u0441\u0438\u043F\u043E\u0432\u0438\u0447\u0441\u043A\u0443\u044E
    \u0432\u043E\u0435\u043D\u043D\u043E-\u043E\u043F\u0435\u0440\u0430\u0442\u0438\u0432\u043D\u0443\u044E
    \u0433\u0440\u0443\u043F\u043F\u0443. \u0423\u0447\u0430\u0441\u0442\u0432\u043E\u0432\u0430\u043B
    \u0432\u043E \u043C\u043D\u043E\u0433\u0438\u0445 \u0431\u043E\u0435\u0432\u044B\u0445
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u044F\u0445. \u041D\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043D
    2 \u043E\u0440\u0434\u0435\u043D\u0430\u043C\u0438 \u041B\u0435\u043D\u0438\u043D\u0430,
    2 \u043E\u0440\u0434\u0435\u043D\u0430\u043C\u0438 \u0422\u0440\u0443\u0434\u043E\u0432\u043E\u0433\u043E
    \u041A\u0440\u0430\u0441\u043D\u043E\u0433\u043E \u0417\u043D\u0430\u043C\u0435\u043D\u0438,
    \u043C\u0435\u0434\u0430\u043B\u044F\u043C\u0438.  \u0421 \u0430\u0432\u0433\u0443\u0441\u0442\u0430
    1944 \u0433. \u043F\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043B\u044C
    \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0441\u043A\u043E\u0433\u043E \u0433\u043E\u0440\u0438\u0441\u043F\u043E\u043B\u043A\u043E\u043C\u0430.
    \u0421 1946 \u0433. \u0433\u0435\u043D\u0435\u0440\u0430\u043B-\u043C\u0430\u0439\u043E\u0440
    \u041D.\u0424. \u041A\u043E\u0440\u043E\u043B\u0451\u0432 \u0432 \u0437\u0430\u043F\u0430\u0441\u0435.
    \u0412 1953\u201357 \u0433\u0433. \u0441\u0435\u043A\u0440\u0435\u0442\u0430\u0440\u044C
    \u0425\u043E\u0442\u0438\u043C\u0441\u043A\u043E\u0433\u043E \u0440\u0430\u0439\u043A\u043E\u043C\u0430
    \u041A\u041F(\u0431)\u0411. \u0412 1956 \u0433. \u043E\u043A\u043E\u043D\u0447\u0438\u043B
    \u0412\u044B\u0441\u0448\u0443\u044E \u043F\u0430\u0440\u0442\u0438\u0439\u043D\u0443\u044E
    \u0448\u043A\u043E\u043B\u0443 \u043F\u0440\u0438 \u0426\u041A \u041A\u041F\u0421\u0421.
    \u041A\u0430\u043D\u0434\u0438\u0434\u0430\u0442 \u0432 \u0447\u043B\u0435\u043D\u044B
    \u0426\u041A \u041A\u041F\u0411 \u0432 1949\u201352 \u0433\u0433., 1954\u201360
    \u0433\u0433. \u0414\u0435\u043F\u0443\u0442\u0430\u0442 \u0412\u0435\u0440\u0445\u043E\u0432\u043D\u043E\u0433\u043E
    \u0421\u043E\u0432\u0435\u0442\u0430 \u0411\u0421\u0421\u0420 \u0432 1947\u201359
    \u0433\u0433. \u0427\u043B\u0435\u043D \u041F\u0440\u0435\u0437\u0438\u0434\u0438\u0443\u043C\u0430
    \u0412\u0435\u0440\u0445\u043E\u0432\u043D\u043E\u0433\u043E \u0421\u043E\u0432\u0435\u0442\u0430
    \u0411\u0421\u0421\u0420 \u0432 1947\u201355 \u0433\u0433. \u0410\u0432\u0442\u043E\u0440
    \u043A\u043D\u0438\u0433\u0438 \"\u0421\u044B\u043D\u043E\u0432\u044C\u044F \u043D\u0430\u0440\u043E\u0434\u0430\"
    (1955).  \u0423\u043C\u0435\u0440 09.04.1972 \u0433. \u041F\u043E\u0445\u043E\u0440\u043E\u043D\u0435\u043D
    \u043D\u0430 \u041C\u0430\u0448\u0435\u043A\u043E\u0432\u0441\u043A\u043E\u043C
    \u043A\u043B\u0430\u0434\u0431\u0438\u0449\u0435 \u0432 \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0435.
    \u041F\u043E\u0447\u0451\u0442\u043D\u044B\u0439 \u0433\u0440\u0430\u0436\u0434\u0430\u043D\u0438\u043D
    \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0430. \u041D\u0430 \u0440\u043E\u0434\u0438\u043D\u0435
    \u041D.\u0424. \u041A\u043E\u0440\u043E\u043B\u0451\u0432\u0443 \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u043B\u0435\u043D
    \u043F\u0430\u043C\u044F\u0442\u043D\u0438\u043A."
