  _name: "\u0413\u0440\u043E\u043C\u043E\u0432\u0430"
  _subjectName: "\u0413\u0440\u043E\u043C\u043E\u0432 \u041C\u0438\u0445\u0430\u0438\u043B
    \u041C\u0438\u0445\u0430\u0439\u043B\u043E\u0432\u0438\u0447"
  _info: "\u0421\u043E\u0432\u0435\u0442\u0441\u043A\u0438\u0439 \u0432\u043E\u0435\u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A,
    \u0433\u0435\u043D\u0435\u0440\u0430\u043B-\u043F\u043E\u043B\u043A\u043E\u0432\u043D\u0438\u043A
    \u0430\u0432\u0438\u0430\u0446\u0438\u0438 (1944), \u0443\u0447\u0430\u0441\u0442\u043D\u0438\u043A
    \u043E\u0441\u0432\u043E\u0431\u043E\u0436\u0434\u0435\u043D\u0438\u044F \u0411\u0435\u043B\u0430\u0440\u0443\u0441\u0438
    \u043E\u0442 \u043D\u0435\u043C\u0435\u0446\u043A\u043E-\u0444\u0430\u0448\u0438\u0441\u0442\u0441\u043A\u0438\u0445
    \u0437\u0430\u0445\u0432\u0430\u0442\u0447\u0438\u043A\u043E\u0432 \u0432 \u0433\u043E\u0434\u044B
    \u0412\u0435\u043B\u0438\u043A\u043E\u0439 \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B, \u0413\u0435\u0440\u043E\u0439 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430 (28.09.1934), \u0437\u0430\u0441\u043B\u0443\u0436\u0435\u043D\u043D\u044B\u0439
    \u043B\u0451\u0442\u0447\u0438\u043A \u0421\u0421\u0421\u0420, \u043F\u0440\u043E\u0444\u0435\u0441\u0441\u043E\u0440
    (1937). \u0420\u043E\u0434\u0438\u043B\u0441\u044F \u0432 \u0433. \u041A\u0430\u043B\u0438\u043D\u0438\u043D.
    \u0427\u043B\u0435\u043D \u041A\u041F\u0421\u0421 \u0441 1941 \u0433. \u041E\u043A\u043E\u043D\u0447\u0438\u043B
    \u0442\u0435\u043E\u0440\u0435\u0442\u0438\u0447\u0435\u0441\u043A\u0438\u0435
    \u043A\u0443\u0440\u0441\u044B \u0438\u043C. \u041D.\u0415. \u0416\u0443\u043A\u043E\u0432\u0441\u043A\u043E\u0433\u043E
    \u043F\u0440\u0438 \u0412\u044B\u0441\u0448\u0435\u043C \u0442\u0435\u0445\u043D\u0438\u0447\u0435\u0441\u043A\u043E\u043C
    \u0443\u0447\u0438\u043B\u0438\u0449\u0435 (1917), \u0448\u043A\u043E\u043B\u0443
    \u043B\u0451\u0442\u0447\u0438\u043A\u043E\u0432 (1918). \u0412 \u041A\u0440\u0430\u0441\u043D\u043E\u0439
    \u0410\u0440\u043C\u0438\u0438 \u0441 1918 \u0433. \u0423\u0447\u0430\u0441\u0442\u043D\u0438\u043A
    \u0413\u0440\u0430\u0436\u0434\u0430\u043D\u0441\u043A\u043E\u0439 \u0432\u043E\u0439\u043D\u044B
    \u043D\u0430 \u0412\u043E\u0441\u0442\u043E\u0447\u043D\u043E\u043C \u0444\u0440\u043E\u043D\u0442\u0435.
    \u041F\u043E\u0441\u043B\u0435 \u0432\u043E\u0439\u043D\u044B \u0438\u043D\u0441\u0442\u0440\u0443\u043A\u0442\u043E\u0440-\u043B\u0451\u0442\u0447\u0438\u043A
    \u0438 \u043B\u0451\u0442\u0447\u0438\u043A-\u0438\u0441\u043F\u044B\u0442\u0430\u0442\u0435\u043B\u044C.
    \u0412 1934 \u0433. \u0437\u0430 \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u043B\u0435\u043D\u0438\u0435
    \u043C\u0438\u0440\u043E\u0432\u043E\u0433\u043E \u0440\u0435\u043A\u043E\u0440\u0434\u0430
    \u0434\u0430\u043B\u044C\u043D\u043E\u0441\u0442\u0438 \u043F\u043E\u043B\u0451\u0442\u0430
    \u043F\u043E \u0437\u0430\u043C\u043A\u043D\u0443\u0442\u043E\u0439 \u043A\u0440\u0438\u0432\u043E\u0439
    \u043D\u0430 \u0440\u0430\u0441\u0441\u0442\u043E\u044F\u043D\u0438\u0438 \u0441\u0432\u044B\u0448\u0435
    12 \u0442\u044B\u0441. \u043A\u0438\u043B\u043E\u043C\u0435\u0442\u0440\u043E\u0432
    \u0435\u043C\u0443 \u043F\u0440\u0438\u0441\u0432\u043E\u0435\u043D\u043E \u0437\u0432\u0430\u043D\u0438\u0435
    \u0413\u0435\u0440\u043E\u044F \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430. \u0412 1937 \u0433. \u0432\u043C\u0435\u0441\u0442\u0435
    \u0441 \u0410.\u0411. \u042E\u043C\u0430\u0448\u0435\u0432\u044B\u043C \u0438
    \u0421.\u0410. \u0414\u0430\u043D\u0438\u043B\u0438\u043D\u044B\u043C \u0441\u043E\u0432\u0435\u0440\u0448\u0438\u043B
    \u043F\u0435\u0440\u0435\u043B\u0451\u0442 \u041C\u043E\u0441\u043A\u0432\u0430
    \u2013 \u0421\u0435\u0432\u0435\u0440\u043D\u044B\u0439 \u043F\u043E\u043B\u044E\u0441
    \u2013 \u0421\u0430\u043D-\u0414\u0436\u0430\u0441\u0438\u043D\u0442\u043E (\u0421\u0428\u0410).
    \u041F\u0435\u0440\u0432\u044B\u0439 \u0438\u0437 \u0441\u043E\u0432\u0435\u0442\u0441\u043A\u0438\u0445
    \u043B\u0451\u0442\u0447\u0438\u043A\u043E\u0432, \u043D\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043D\u043D\u044B\u0445
    \u041C\u0435\u0436\u0434\u0443\u043D\u0430\u0440\u043E\u0434\u043D\u043E\u0439
    \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0439 \u0444\u0435\u0434\u0435\u0440\u0430\u0446\u0438\u0435\u0439
    \u043C\u0435\u0434\u0430\u043B\u044C\u044E \u0434\u0435 \u041B\u0430\u0432\u043E
    (1937). \u0411\u044B\u043B \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440\u043E\u043C
    \u0430\u0432\u0438\u0430\u043E\u0442\u0440\u044F\u0434\u0430 \u0426\u0435\u043D\u0442\u0440\u0430\u043B\u044C\u043D\u043E\u0433\u043E
    \u0430\u044D\u0440\u043E\u0433\u0438\u0434\u0440\u043E\u0434\u0438\u043D\u0430\u043C\u0438\u0447\u0435\u0441\u043A\u043E\u0433\u043E
    \u0438\u043D\u0441\u0442\u0438\u0442\u0443\u0442\u0430 \u0438\u043C. \u041D.\u0415.
    \u0416\u0443\u043A\u043E\u0432\u0441\u043A\u043E\u0433\u043E. \u0412 1940\u201341
    \u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A \u041B\u0451\u0442\u043D\u043E-\u0438\u0441\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u0442\u0435\u043B\u044C\u0441\u043A\u043E\u0433\u043E
    \u0438\u043D\u0441\u0442\u0438\u0442\u0443\u0442\u0430 \u041D\u0430\u0440\u043A\u043E\u043C\u0430\u0442\u0430
    \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0439 \u043F\u0440\u043E\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u043E\u0441\u0442\u0438
    \u0421\u0421\u0421\u0420. \u0421 \u0434\u0435\u043A\u0430\u0431\u0440\u044F 1941
    \u0433. \u0432 \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044E\u0449\u0435\u0439
    \u0430\u0440\u043C\u0438\u0438: \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440
    \u0430\u0432\u0438\u0430\u0434\u0438\u0432\u0438\u0437\u0438\u0438, \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0438\u0439
    \u0412\u0412\u0421 \u041A\u0430\u043B\u0438\u043D\u0438\u043D\u0441\u043A\u043E\u0433\u043E
    \u0444\u0440\u043E\u043D\u0442\u0430, \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0438\u0439
    3-\u0439 \u0438 1-\u0439 \u0432\u043E\u0437\u0434\u0443\u0448\u043D\u044B\u043C\u0438
    \u0430\u0440\u043C\u0438\u044F\u043C\u0438. \u041E\u0431\u0435\u0441\u043F\u0435\u0447\u0438\u0432\u0430\u043B
    \u0432\u043E\u0437\u0434\u0443\u0448\u043D\u0443\u044E \u043F\u043E\u0434\u0434\u0435\u0440\u0436\u043A\u0443
    \u0438 \u043F\u0440\u0438\u043A\u0440\u044B\u0442\u0438\u0435 \u0441\u043E\u0432\u0435\u0442\u0441\u043A\u0438\u0445
    \u0432\u043E\u0439\u0441\u043A \u0441 \u0432\u043E\u0437\u0434\u0443\u0445\u0430
    \u0432 \u0431\u0438\u0442\u0432\u0430\u0445 \u043F\u043E\u0434 \u041C\u043E\u0441\u043A\u0432\u043E\u0439
    \u0438 \u041A\u0443\u0440\u0441\u043A\u043E\u043C, \u0432 \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u044F\u0445
    \u043F\u043E \u043E\u0441\u0432\u043E\u0431\u043E\u0436\u0434\u0435\u043D\u0438\u044E
    \u0411\u0435\u043B\u0430\u0440\u0443\u0441\u0438. \u0421 \u0438\u044E\u043D\u044F
    1944 \u0433. \u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A \u0413\u043B\u0430\u0432\u043D\u043E\u0433\u043E
    \u0443\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u0438\u044F \u0431\u043E\u0435\u0432\u043E\u0439
    \u043F\u043E\u0434\u0433\u043E\u0442\u043E\u0432\u043A\u0438 \u0444\u0440\u043E\u043D\u0442\u043E\u0432\u043E\u0439
    \u0430\u0432\u0438\u0430\u0446\u0438\u0438. \u0412 1946\u201349 \u0433\u0433.
    \u0437\u0430\u043C\u0435\u0441\u0442\u0438\u0442\u0435\u043B\u044C \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0435\u0433\u043E
    \u0434\u0430\u043B\u044C\u043D\u0435\u0439 \u0430\u0432\u0438\u0430\u0446\u0438\u0435\u0439,
    \u0437\u0430\u0442\u0435\u043C \u043D\u0430 \u0440\u0430\u0431\u043E\u0442\u0435
    \u0432 \u041C\u0438\u043D\u0438\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0435
    \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0439 \u043F\u0440\u043E\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u043E\u0441\u0442\u0438
    \u0421\u0421\u0421\u0420. \u0414\u0435\u043F\u0443\u0442\u0430\u0442 \u0412\u0435\u0440\u0445\u043E\u0432\u043D\u043E\u0433\u043E
    \u0421\u043E\u0432\u0435\u0442\u0430 \u0421\u0421\u0421\u0420 \u0432 1937\u201350
    \u0433\u0433. \u0412 \u0433\u043E\u0434\u044B \u0412\u0435\u043B\u0438\u043A\u043E\u0439
    \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B \u0435\u0433\u043E \u0438\u043C\u044F \u0431\u044B\u043B\u043E
    \u043F\u0440\u0438\u0441\u0432\u043E\u0435\u043D\u043E 2 \u043F\u0430\u0440\u0442\u0438\u0437\u0430\u043D\u0441\u043A\u0438\u043C
    \u043E\u0442\u0440\u044F\u0434\u0430\u043C, \u0434\u0435\u0439\u0441\u0442\u0432\u043E\u0432\u0430\u0432\u0448\u0438\u043C
    \u043D\u0430 \u043E\u043A\u043A\u0443\u043F\u0438\u0440\u043E\u0432\u0430\u043D\u043D\u043E\u0439
    \u0442\u0435\u0440\u0440\u0438\u0442\u043E\u0440\u0438\u0438 \u041C\u0438\u043D\u0441\u043A\u043E\u0439
    \u043E\u0431\u043B\u0430\u0441\u0442\u0438."
