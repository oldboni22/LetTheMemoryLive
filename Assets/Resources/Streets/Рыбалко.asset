  m_Name: "\u0420\u044B\u0431\u0430\u043B\u043A\u043E"
  m_EditorClassIdentifier: 
  _name: "\u0420\u044B\u0431\u0430\u043B\u043A\u043E"
  _subjectName: "\u0420\u044B\u0431\u0430\u043B\u043A\u043E \u041F\u0430\u0432\u0435\u043B
    \u0421\u0435\u043C\u0451\u043D\u043E\u0432\u0438\u0447"
  _info: "\u0421\u043E\u0432\u0435\u0442\u0441\u043A\u0438\u0439 \u0432\u043E\u0435\u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A,
    \u043C\u0430\u0440\u0448\u0430\u043B \u0431\u0440\u043E\u043D\u0435\u0442\u0430\u043D\u043A\u043E\u0432\u044B\u0445
    \u0432\u043E\u0439\u0441\u043A (1945), \u0434\u0432\u0430\u0436\u0434\u044B \u0413\u0435\u0440\u043E\u0439
    \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E \u0421\u043E\u044E\u0437\u0430
    (17.11.1943 \u0438 06.04.1945). \u0420\u043E\u0434\u0438\u043B\u0441\u044F \u0432
    \u0441. \u041C\u0430\u043B\u044B\u0439 \u0418\u0441\u0442\u043E\u0440\u043E\u043F
    \u041B\u0435\u0431\u0435\u0434\u0438\u043D\u0441\u043A\u043E\u0433\u043E \u0440\u0430\u0439\u043E\u043D\u0430
    \u0421\u0443\u043C\u0441\u043A\u043E\u0439 \u043E\u0431\u043B\u0430\u0441\u0442\u0438.
    \u0427\u043B\u0435\u043D \u041A\u041F\u0421\u0421 \u0441 1919 \u0433. \u0412
    \u0430\u0440\u043C\u0438\u0438 \u0441 1914 \u0433. \u0423\u0447\u0430\u0441\u0442\u043D\u0438\u043A
    1-\u0439 \u043C\u0438\u0440\u043E\u0432\u043E\u0439 \u0432\u043E\u0439\u043D\u044B.
    \u0421 \u0434\u0435\u043A\u0430\u0431\u0440\u044F 1917 \u0433. \u0432 \u041A\u0440\u0430\u0441\u043D\u043E\u0439
    \u0433\u0432\u0430\u0440\u0434\u0438\u0438, \u0441 1919 \u0433. \u0432 \u041A\u0440\u0430\u0441\u043D\u043E\u0439
    \u0410\u0440\u043C\u0438\u0438. \u0412\u043E \u0432\u0440\u0435\u043C\u044F \u0413\u0440\u0430\u0436\u0434\u0430\u043D\u0441\u043A\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B \u043A\u043E\u043C\u0438\u0441\u0441\u0430\u0440
    \u043F\u043E\u043B\u043A\u0430 \u0438 \u0431\u0440\u0438\u0433\u0430\u0434\u044B
    \u0432 1-\u0439 \u041A\u043E\u043D\u043D\u043E\u0439 \u0430\u0440\u043C\u0438\u0438,
    \u0432\u043E\u0435\u0432\u0430\u043B \u043F\u0440\u043E\u0442\u0438\u0432 \u043D\u0435\u043C\u0435\u0446\u043A\u0438\u0445
    \u043E\u043A\u043A\u0443\u043F\u0430\u043D\u0442\u043E\u0432, \u043F\u0435\u0442\u043B\u044E\u0440\u043E\u0432\u0446\u0435\u0432,
    \u0432\u043E\u0439\u0441\u043A \u0414\u0435\u043D\u0438\u043A\u0438\u043D\u0430,
    \u0412\u0440\u0430\u043D\u0433\u0435\u043B\u044F, \u0431\u0443\u0440\u0436\u0443\u0430\u0437\u043D\u043E\u0439
    \u041F\u043E\u043B\u044C\u0448\u0438. \u041E\u043A\u043E\u043D\u0447\u0438\u043B
    \u043A\u0443\u0440\u0441\u044B \u0443\u0441\u043E\u0432\u0435\u0440\u0448\u0435\u043D\u0441\u0442\u0432\u043E\u0432\u0430\u043D\u0438\u044F
    \u0432\u044B\u0441\u0448\u0435\u0433\u043E \u043D\u0430\u0447\u0441\u043E\u0441\u0442\u0430\u0432\u0430
    (1926 \u0438 1930), \u0412\u043E\u0435\u043D\u043D\u0443\u044E \u0430\u043A\u0430\u0434\u0435\u043C\u0438\u044E
    \u0438\u043C. \u041C.\u0412. \u0424\u0440\u0443\u043D\u0437\u0435 (1934). \u0421
    1926 \u0433. \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440 \u044D\u0441\u043A\u0430\u0434\u0440\u043E\u043D\u0430,
    \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440 \u0438 \u043A\u043E\u043C\u0438\u0441\u0441\u0430\u0440
    \u043F\u043E\u043B\u043A\u0430 \u0438 \u043A\u0430\u0432\u0430\u043B\u0435\u0440\u0438\u0439\u0441\u043A\u043E\u0439
    \u0431\u0440\u0438\u0433\u0430\u0434\u044B, \u043F\u043E\u043C\u043E\u0449\u043D\u0438\u043A
    \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440\u0430 \u0433\u043E\u0440\u043D\u043E\u043A\u0430\u0432\u0430\u043B\u0435\u0440\u0438\u0439\u0441\u043A\u043E\u0439
    \u0434\u0438\u0432\u0438\u0437\u0438\u0438. \u0421 1937 \u0433. \u0432\u043E\u0435\u043D\u043D\u044B\u0439
    \u0430\u0442\u0442\u0430\u0448\u0435 \u0432 \u041F\u043E\u043B\u044C\u0448\u0435
    \u0438 \u041A\u0438\u0442\u0430\u0435, \u043D\u0430 \u043F\u0440\u0435\u043F\u043E\u0434\u0430\u0432\u0430\u0442\u0435\u043B\u044C\u0441\u043A\u043E\u0439
    \u0440\u0430\u0431\u043E\u0442\u0435. \u0412 \u0433\u043E\u0434\u044B \u0412\u0435\u043B\u0438\u043A\u043E\u0439
    \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B \u0441 \u043C\u0430\u044F 1942 \u0433. \u0437\u0430\u043C\u0435\u0441\u0442\u0438\u0442\u0435\u043B\u044C
    \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0435\u0433\u043E, \u0441
    \u0438\u044E\u043B\u044F 1942 \u0433. \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0438\u0439
    \u0442\u0430\u043D\u043A\u043E\u0432\u044B\u043C\u0438 \u0430\u0440\u043C\u0438\u044F\u043C\u0438
    \u043D\u0430 \u0411\u0440\u044F\u043D\u0441\u043A\u043E\u043C, \u042E\u0433\u043E-\u0417\u0430\u043F\u0430\u0434\u043D\u043E\u043C,
    \u0426\u0435\u043D\u0442\u0440\u0430\u043B\u044C\u043D\u043E\u043C, \u0412\u043E\u0440\u043E\u043D\u0435\u0436\u0441\u043A\u043E\u043C,
    1-\u043C \u0411\u0435\u043B\u043E\u0440\u0443\u0441\u0441\u043A\u043E\u043C \u0438
    1-\u043C \u0423\u043A\u0440\u0430\u0438\u043D\u0441\u043A\u043E\u043C \u0444\u0440\u043E\u043D\u0442\u0430\u0445.
    \u0412\u043E\u0439\u0441\u043A\u0430 \u043F\u043E\u0434 \u0435\u0433\u043E \u043A\u043E\u043C\u0430\u043D\u0434\u043E\u0432\u0430\u043D\u0438\u0435\u043C
    \u0443\u0441\u043F\u0435\u0448\u043D\u043E \u0443\u0447\u0430\u0441\u0442\u0432\u043E\u0432\u0430\u043B\u0438
    \u0432 \u041E\u0441\u0442\u0440\u043E\u0433\u043E\u0436\u0441\u043A\u043E-\u0420\u043E\u0441\u0441\u043E\u0448\u0430\u043D\u0441\u043A\u043E\u0439,
    \u0425\u0430\u0440\u044C\u043A\u043E\u0432\u0441\u043A\u043E\u0439 \u043D\u0430\u0441\u0442\u0443\u043F\u0430\u0442\u0435\u043B\u044C\u043D\u043E\u0439
    (1943) \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u044F\u0445, \u0432 \u0431\u0438\u0442\u0432\u0435
    \u043F\u043E\u0434 \u041A\u0443\u0440\u0441\u043A\u043E\u043C, \u041A\u0438\u0435\u0432\u0441\u043A\u043E\u0439
    \u043D\u0430\u0441\u0442\u0443\u043F\u0430\u0442\u0435\u043B\u044C\u043D\u043E\u0439,
    \u0416\u0438\u0442\u043E\u043C\u0438\u0440\u0441\u043A\u043E-\u0411\u0435\u0440\u0434\u0438\u0447\u0435\u0432\u0441\u043A\u043E\u0439,
    \u041F\u0440\u043E\u0441\u043A\u0443\u0440\u043E\u0432\u0441\u043A\u043E-\u0427\u0435\u0440\u043D\u043E\u0432\u0438\u0446\u043A\u043E\u0439,
    \u041B\u044C\u0432\u043E\u0432\u0441\u043A\u043E-\u0421\u0430\u043D\u0434\u043E\u043C\u0438\u0440\u0441\u043A\u043E\u0439,
    \u041D\u0438\u0436\u043D\u0435-\u0421\u0438\u043B\u0435\u0437\u0441\u043A\u043E\u0439,
    \u0412\u0435\u0440\u0445\u043D\u0435-\u0421\u0438\u043B\u0435\u0437\u0441\u043A\u043E\u0439,
    \u0411\u0435\u0440\u043B\u0438\u043D\u0441\u043A\u043E\u0439 \u0438 \u041F\u0440\u0430\u0436\u0441\u043A\u043E\u0439
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u044F\u0445. \u0417\u0430 \u0443\u043C\u0435\u043B\u043E\u0435
    \u0440\u0443\u043A\u043E\u0432\u043E\u0434\u0441\u0442\u0432\u043E \u0430\u0440\u043C\u0438\u0435\u0439
    \u0432 \u0431\u0438\u0442\u0432\u0435 \u043F\u043E\u0434 \u041A\u0443\u0440\u0441\u043A\u043E\u043C
    \u0438 \u0432 \u041A\u0438\u0435\u0432\u0441\u043A\u043E\u0439 \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u0438,
    \u0443\u0441\u043F\u0435\u0448\u043D\u043E\u0435 \u0444\u043E\u0440\u0441\u0438\u0440\u043E\u0432\u0430\u043D\u0438\u0435
    \u0414\u043D\u0435\u043F\u0440\u0430 \u0438 \u043F\u0440\u043E\u044F\u0432\u043B\u0435\u043D\u043D\u043E\u0435
    \u043F\u0440\u0438 \u044D\u0442\u043E\u043C \u043C\u0443\u0436\u0435\u0441\u0442\u0432\u043E
    \u0438 \u0433\u0435\u0440\u043E\u0438\u0437\u043C \u0435\u043C\u0443 \u043F\u0440\u0438\u0441\u0432\u043E\u0435\u043D\u043E
    \u0437\u0432\u0430\u043D\u0438\u0435 \u0413\u0435\u0440\u043E\u044F \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430. \u0412\u0442\u043E\u0440\u043E\u0439 \u043C\u0435\u0434\u0430\u043B\u044C\u044E
    \"\u0417\u043E\u043B\u043E\u0442\u0430\u044F \u0417\u0432\u0435\u0437\u0434\u0430\"
    \u043D\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043D \u0437\u0430 \u0431\u043E\u0435\u0432\u044B\u0435
    \u043E\u0442\u043B\u0438\u0447\u0438\u044F \u0432\u043E\u0439\u0441\u043A \u043F\u043E\u0434
    \u0435\u0433\u043E \u043A\u043E\u043C\u0430\u043D\u0434\u043E\u0432\u0430\u043D\u0438\u0435\u043C
    \u043D\u0430 \u0437\u0430\u0432\u0435\u0440\u0448\u0430\u044E\u0449\u0435\u043C
    \u044D\u0442\u0430\u043F\u0435 \u0432\u043E\u0439\u043D\u044B \u0438 \u043F\u0440\u043E\u044F\u0432\u043B\u0435\u043D\u043D\u044B\u0439
    \u043B\u0438\u0447\u043D\u044B\u0439 \u0433\u0435\u0440\u043E\u0438\u0437\u043C.
    \u0421 1946 \u0433. \u043F\u0435\u0440\u0432\u044B\u0439 \u0437\u0430\u043C\u0435\u0441\u0442\u0438\u0442\u0435\u043B\u044C
    \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0435\u0433\u043E, \u0441
    1947 \u0433. \u043A\u043E\u043C\u0430\u043D\u0434\u0443\u044E\u0449\u0438\u0439
    \u0431\u0440\u043E\u043D\u0435\u0442\u0430\u043D\u043A\u043E\u0432\u044B\u043C\u0438
    \u0438 \u043C\u0435\u0445\u0430\u043D\u0438\u0437\u0438\u0440\u043E\u0432\u0430\u043D\u043D\u044B\u043C\u0438
    \u0432\u043E\u0439\u0441\u043A\u0430\u043C\u0438 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0439
    \u0410\u0440\u043C\u0438\u0438. \u0414\u0435\u043F\u0443\u0442\u0430\u0442 \u0412\u0435\u0440\u0445\u043E\u0432\u043D\u043E\u0433\u043E
    \u0421\u043E\u0432\u0435\u0442\u0430 \u0421\u0421\u0421\u0420 \u0432 1946\u201348.
    \u0421\u043A\u043E\u043D\u0447\u0430\u043B\u0441\u044F 28 \u0430\u0432\u0433\u0443\u0441\u0442\u0430
    1948 \u0433. \u041F\u043E\u0445\u043E\u0440\u043E\u043D\u0435\u043D \u043D\u0430
    \u041D\u043E\u0432\u043E\u0434\u0435\u0432\u0438\u0447\u044C\u0435\u043C \u043A\u043B\u0430\u0434\u0431\u0438\u0449\u0435
    \u0432 \u041C\u043E\u0441\u043A\u0432\u0435."
