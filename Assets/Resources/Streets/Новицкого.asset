  m_EditorClassIdentifier: 
  _name: "\u041D\u043E\u0432\u0438\u0446\u043A\u043E\u0433\u043E"
  _subjectName: "\u041D\u043E\u0432\u0438\u0446\u043A\u0438\u0439 \u041D\u0438\u043A\u043E\u043B\u0430\u0439
    \u041C\u0438\u0445\u0430\u0439\u043B\u043E\u0432\u0438\u0447"
  _info: "\u0423\u0447\u0430\u0441\u0442\u043D\u0438\u043A \u0412\u0435\u043B\u0438\u043A\u043E\u0439
    \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439
    \u0432\u043E\u0439\u043D\u044B, \u0413\u0435\u0440\u043E\u0439 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430 (31.03.1943). \u0420\u043E\u0434\u0438\u043B\u0441\u044F
    \u0432 \u0433. \u0412\u0435\u043B\u0438\u0436 \u0421\u043C\u043E\u043B\u0435\u043D\u0441\u043A\u043E\u0439
    \u043E\u0431\u043B\u0430\u0441\u0442\u0438. \u0427\u043B\u0435\u043D \u0412\u041B\u041A\u0421\u041C.
    \u0414\u0435\u0442\u0441\u043A\u0438\u0435 \u0438 \u044E\u043D\u043E\u0448\u0435\u0441\u043A\u0438\u0435
    \u0433\u043E\u0434\u044B \u043F\u0440\u043E\u0448\u043B\u0438 \u0432 \u0433.
    \u0423\u0437\u0434\u0430 \u041C\u0438\u043D\u0441\u043A\u043E\u0439 \u043E\u0431\u043B\u0430\u0441\u0442\u0438.
    \u0421 1934 \u0433. \u0432 \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0435:
    \u043C\u043E\u043D\u0442\u0451\u0440 \u043D\u0430 \u044D\u043B\u0435\u043A\u0442\u0440\u043E\u0441\u0442\u0430\u043D\u0446\u0438\u0438,
    \u044D\u043B\u0435\u043A\u0442\u0440\u043E\u043C\u0435\u0445\u0430\u043D\u0438\u043A
    \u0436\u0435\u043B\u0435\u0437\u043D\u043E\u0434\u043E\u0440\u043E\u0436\u043D\u043E\u0433\u043E
    \u0434\u0435\u043F\u043E. \u0412 \u041A\u0440\u0430\u0441\u043D\u043E\u0439 \u0410\u0440\u043C\u0438\u0438
    \u0441 1940 \u0433. \u0421 1941 \u0433. \u043D\u0430 \u0417\u0430\u043F\u0430\u0434\u043D\u043E\u043C,
    \u0421\u0435\u0432\u0435\u0440\u043E-\u041A\u0430\u0432\u043A\u0430\u0437\u0441\u043A\u043E\u043C
    \u0438 \u0417\u0430\u043A\u0430\u0432\u043A\u0430\u0437\u0441\u043A\u043E\u043C
    \u0444\u0440\u043E\u043D\u0442\u0430\u0445.  \u041A\u043E\u043C\u0430\u043D\u0434\u0438\u0440
    \u043E\u0442\u0434\u0435\u043B\u0435\u043D\u0438\u044F \u041D.\u041C. \u041D\u043E\u0432\u0438\u0446\u043A\u0438\u0439
    \u043E\u0441\u043E\u0431\u043E \u043E\u0442\u043B\u0438\u0447\u0438\u043B\u0441\u044F
    \u0432 \u0431\u043E\u044F\u0445 \u043D\u0430 \u043F\u043E\u0434\u0441\u0442\u0443\u043F\u0430\u0445
    \u043A \u0433. \u0422\u0443\u0430\u043F\u0441\u0435. 10.10.1942 \u0433. \u0435\u0433\u043E
    \u043E\u0442\u0434\u0435\u043B\u0435\u043D\u0438\u0435 \u0448\u0442\u0443\u0440\u043C\u043E\u0432\u0430\u043B\u043E
    \u0432\u044B\u0441\u043E\u0442\u0443 \u0443 \u0441\u0442. \u041A\u0443\u0440\u0438\u043D\u0441\u043A\u0430\u044F.
    \u041F\u0435\u0440\u0435\u0434 \u044D\u0442\u0438\u043C \u0431\u043E\u0435\u043C
    \u043E\u043D \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u043B \u0440\u043E\u0434\u043D\u044B\u043C
    \u043A\u043E\u0440\u043E\u0442\u043A\u043E\u0435 \u043F\u0438\u0441\u044C\u043C\u043E,
    \u0432 \u043A\u043E\u0442\u043E\u0440\u043E\u043C \u0441\u043E\u043E\u0431\u0449\u0430\u043B:
    \"...\u0421\u0435\u0433\u043E\u0434\u043D\u044F \u0432\u0441\u0435\u0445 \u043D\u0430\u0441,
    \u043A\u0442\u043E \u043E\u0441\u0442\u0430\u043B\u0441\u044F \u0436\u0438\u0432\u044B\u043C,
    \u043F\u043E\u0441\u0442\u0440\u043E\u0438\u043B \u043A\u043E\u043C\u0430\u043D\u0434\u0438\u0440
    \u0440\u043E\u0442\u044B \u0438 \u0441\u043A\u0430\u0437\u0430\u043B, \u0447\u0442\u043E,
    \u0432\u0438\u0434\u043D\u043E, \u043D\u0438\u043A\u0442\u043E \u0438\u0437 \u043D\u0430\u0441
    \u0436\u0438\u0432\u043E\u0439 \u043E\u0442\u0441\u044E\u0434\u0430 \u043D\u0435
    \u0432\u0435\u0440\u043D\u0451\u0442\u0441\u044F. \u0422\u043E\u0442, \u043A\u0442\u043E
    \u0433\u043E\u0442\u043E\u0432 \u0443\u043C\u0435\u0440\u0435\u0442\u044C \u0437\u0430
    \u0420\u043E\u0434\u0438\u043D\u0443, \u043F\u0443\u0441\u043A\u0430\u0439 \u0441\u0434\u0435\u043B\u0430\u0435\u0442
    \u0448\u0430\u0433 \u0432\u043F\u0435\u0440\u0451\u0434. \u041C\u044B \u0441\u0434\u0435\u043B\u0430\u043B\u0438
    \u0432\u0441\u0435 27... \u042F \u043F\u043E\u0433\u0438\u0431\u043D\u0443, \u043C\u043E\u0436\u0435\u0442
    \u0431\u044B\u0442\u044C, \u0447\u0435\u0440\u0435\u0437 \u0434\u0432\u0430 \u0447\u0430\u0441\u0430.
    \u041D\u043E \u0444\u0430\u0448\u0438\u0441\u0442\u044B \u043D\u0435 \u043F\u0440\u043E\u0439\u0434\u0443\u0442\". 
    \u0413\u0432\u0430\u0440\u0434\u0435\u0439\u0446\u044B \u0432\u043E\u0440\u0432\u0430\u043B\u0438\u0441\u044C
    \u0432 \u0442\u0440\u0430\u043D\u0448\u0435\u0438 \u043F\u0440\u043E\u0442\u0438\u0432\u043D\u0438\u043A\u0430
    \u0438 \u0432 \u0440\u0443\u043A\u043E\u043F\u0430\u0448\u043D\u043E\u0439 \u0441\u0445\u0432\u0430\u0442\u043A\u0435
    \u0437\u0430\u0445\u0432\u0430\u0442\u0438\u043B\u0438 4 \u043F\u0443\u043B\u0435\u043C\u0451\u0442\u0430,
    \u0440\u0430\u0434\u0438\u043E\u0441\u0442\u0430\u043D\u0446\u0438\u044E. \u041A\u043E\u0433\u0434\u0430
    \u0434\u043E \u0433\u0440\u0435\u0431\u043D\u044F \u043E\u0441\u0442\u0430\u0432\u0430\u043B\u043E\u0441\u044C
    20\u201330 \u043C\u0435\u0442\u0440\u043E\u0432, \u043D\u0435\u043E\u0436\u0438\u0434\u0430\u043D\u043D\u043E
    \u0438\u0437 \u0434\u0437\u043E\u0442\u0430 \u0443\u0434\u0430\u0440\u0438\u043B
    \u0441\u0442\u0430\u043D\u043A\u043E\u0432\u044B\u0439 \u043F\u0443\u043B\u0435\u043C\u0451\u0442
    \u043F\u0440\u043E\u0442\u0438\u0432\u043D\u0438\u043A\u0430, \u043F\u0440\u0435\u0433\u0440\u0430\u0434\u0438\u0432
    \u043F\u0443\u0442\u044C \u043D\u0430\u0448\u0438\u043C \u0432\u043E\u0438\u043D\u0430\u043C.
    \u041F\u043E\u0434 \u043E\u0433\u043D\u0451\u043C \u043F\u0440\u043E\u0442\u0438\u0432\u043D\u0438\u043A\u0430
    \u041D.\u041C. \u041D\u043E\u0432\u0438\u0446\u043A\u0438\u0439 \u043F\u043E\u0434\u043F\u043E\u043B\u0437
    \u043A \u0434\u0437\u043E\u0442\u0443 \u0438 \u0431\u0440\u043E\u0441\u0438\u043B
    \u0434\u0432\u0435 \u0433\u0440\u0430\u043D\u0430\u0442\u044B. \u041F\u0443\u043B\u0435\u043C\u0451\u0442
    \u0443\u043C\u043E\u043B\u043A, \u043D\u043E \u0432\u0441\u043A\u043E\u0440\u0435
    \u0437\u0430\u0441\u0442\u0440\u043E\u0447\u0438\u043B \u0441\u043D\u043E\u0432\u0430.
    \u0422\u043E\u0433\u0434\u0430 \u0433\u0432\u0430\u0440\u0434\u0435\u0435\u0446
    \u0440\u0432\u0430\u043D\u0443\u043B\u0441\u044F \u043A \u0434\u0437\u043E\u0442\u0443
    \u0438 \u0442\u0435\u043B\u043E\u043C \u0437\u0430\u043A\u0440\u044B\u043B \u0435\u0433\u043E
    \u0430\u043C\u0431\u0440\u0430\u0437\u0443\u0440\u0443, \u043E\u0431\u0435\u0441\u043F\u0435\u0447\u0438\u0432
    \u0443\u0441\u043F\u0435\u0445 \u043D\u0430\u0441\u0442\u0443\u043F\u043B\u0435\u043D\u0438\u044F
    \u0441\u0432\u043E\u0435\u0433\u043E \u043E\u0442\u0434\u0435\u043B\u0435\u043D\u0438\u044F."
