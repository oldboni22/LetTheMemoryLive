  m_Name: "\u041D\u0430\u0434\u0442\u043E\u0447\u0435\u0435\u0432\u0430"
  m_EditorClassIdentifier: 
  _name: "\u041D\u0430\u0434\u0442\u043E\u0447\u0435\u0435\u0432\u0430"
  _subjectName: "\u041D\u0430\u0434\u0442\u043E\u0447\u0435\u0435\u0432 \u0413\u0435\u043E\u0440\u0433\u0438\u0439
    \u041C\u0435\u0444\u043E\u0434\u0438\u0435\u0432\u0438\u0447"
  _info: "\u0435\u0440\u043E\u0439 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430 (13.04.1944).  \u0420\u043E\u0434\u0438\u043B\u0441\u044F
    \u0432 \u043D\u043E\u044F\u0431\u0440\u0435 1916 \u0433. \u0432 \u0411\u0435\u043B\u0430\u0440\u0443\u0441\u0438,
    \u0432 \u0434\u0435\u0440\u0435\u0432\u043D\u0435 \u041A\u0440\u0430\u0441\u043D\u0438\u0446\u0430
    1 \u0411\u044B\u0445\u043E\u0432\u0441\u043A\u043E\u0433\u043E \u0440\u0430\u0439\u043E\u043D\u0430
    \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0441\u043A\u043E\u0439 \u043E\u0431\u043B\u0430\u0441\u0442\u0438
    \u0432 \u043A\u0440\u0435\u0441\u0442\u044C\u044F\u043D\u0441\u043A\u043E\u0439
    \u0441\u0435\u043C\u044C\u0435.  \u041E\u043A\u043E\u043D\u0447\u0438\u043B \u0448\u043A\u043E\u043B\u0443
    \u0444\u0430\u0431\u0440\u0438\u0447\u043D\u043E-\u0437\u0430\u0432\u043E\u0434\u0441\u043A\u043E\u0433\u043E
    \u0443\u0447\u0435\u043D\u0438\u0447\u0435\u0441\u0442\u0432\u0430. \u0420\u0430\u0431\u043E\u0442\u0430\u043B
    \u044D\u043B\u0435\u043A\u0442\u0440\u043E\u043C\u043E\u043D\u0442\u0451\u0440\u043E\u043C
    \u043D\u0430 \u0436\u0435\u043B\u0435\u0437\u043D\u043E\u0439 \u0434\u043E\u0440\u043E\u0433\u0435
    \u0432 \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0435, \u0443\u0447\u0438\u043B\u0441\u044F
    \u043D\u0430 \u0440\u0430\u0431\u0444\u0430\u043A\u0435. \u041E\u043A\u043E\u043D\u0447\u0438\u043B
    \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0441\u043A\u0438\u0439 \u0430\u044D\u0440\u043E\u043A\u043B\u0443\u0431
    (1940). \u0412 \u041A\u0440\u0430\u0441\u043D\u043E\u0439 \u0410\u0440\u043C\u0438\u0438
    \u0441 1941 \u0433., \u043E\u043A\u043E\u043D\u0447\u0438\u043B \u0432\u043E\u0435\u043D\u043D\u043E\u0435
    \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0435 \u0443\u0447\u0438\u043B\u0438\u0449\u0435. 
    \u0412 \u0412\u0435\u043B\u0438\u043A\u0443\u044E \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u0443\u044E
    \u0432\u043E\u0439\u043D\u0443 \u0441 \u0438\u044E\u043D\u044F 1941 \u0433. \u0432
    \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044E\u0449\u0435\u0439 \u0430\u0440\u043C\u0438\u0438,
    \u0441\u043E\u0432\u0435\u0440\u0448\u0438\u043B \u043E\u043A\u043E\u043B\u043E
    100 \u0431\u043E\u0435\u0432\u044B\u0445 \u0432\u044B\u043B\u0435\u0442\u043E\u0432.
    \u041B\u0451\u0442\u0447\u0438\u043A 76-\u0433\u043E \u0448\u0442\u0443\u0440\u043C\u043E\u0432\u043E\u0433\u043E
    \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0433\u043E \u043F\u043E\u043B\u043A\u0430
    (1-\u044F \u0433\u0432\u0430\u0440\u0434\u0435\u0439\u0441\u043A\u0430\u044F
    \u0448\u0442\u0443\u0440\u043C\u043E\u0432\u0430\u044F \u0430\u0432\u0438\u0430\u0446\u0438\u043E\u043D\u043D\u0430\u044F
    \u0434\u0438\u0432\u0438\u0437\u0438\u044F, 8-\u044F \u0432\u043E\u0437\u0434\u0443\u0448\u043D\u0430\u044F
    \u0430\u0440\u043C\u0438\u044F, 4-\u0439 \u0423\u043A\u0440\u0430\u0438\u043D\u0441\u043A\u0438\u0439
    \u0444\u0440\u043E\u043D\u0442) \u0433\u0432\u0430\u0440\u0434\u0438\u0438 \u043C\u043B\u0430\u0434\u0448\u0438\u0439
    \u043B\u0435\u0439\u0442\u0435\u043D\u0430\u043D\u0442 \u0413.\u041C. \u041D\u0430\u0434\u0442\u043E\u0447\u0435\u0435\u0432
    31.01.1944 \u0433. \u043F\u0440\u0438\u0437\u0435\u043C\u043B\u0438\u043B\u0441\u044F
    \u043D\u0430 \u0442\u0435\u0440\u0440\u0438\u0442\u043E\u0440\u0438\u0438 \u043F\u0440\u043E\u0442\u0438\u0432\u043D\u0438\u043A\u0430,
    \u0447\u0442\u043E\u0431\u044B \u0441\u043F\u0430\u0441\u0442\u0438 \u044D\u043A\u0438\u043F\u0430\u0436
    \u0441\u0432\u043E\u0435\u0433\u043E \u0432\u0435\u0434\u0443\u0449\u0435\u0433\u043E.
    \u041F\u0443\u0448\u0435\u0447\u043D\u043E-\u043F\u0443\u043B\u0435\u043C\u0451\u0442\u043D\u044B\u043C
    \u043E\u0433\u043D\u0451\u043C \u0440\u0430\u0437\u043E\u0433\u043D\u0430\u043B
    \u043F\u0440\u0438\u0431\u043B\u0438\u0436\u0430\u0432\u0448\u0438\u0445\u0441\u044F
    \u043A \u0441\u0430\u043C\u043E\u043B\u0451\u0442\u0443 \u0433\u0438\u0442\u043B\u0435\u0440\u043E\u0432\u0446\u0435\u0432,
    \u0432\u0437\u044F\u043B \u044D\u043A\u0438\u043F\u0430\u0436 \u043F\u043E\u0434\u0431\u0438\u0442\u043E\u0433\u043E
    \u0441\u0430\u043C\u043E\u043B\u0451\u0442\u0430 \u043D\u0430 \u0431\u043E\u0440\u0442
    \u0438 \u0432\u0435\u0440\u043D\u0443\u043B\u0441\u044F \u043D\u0430 \u0441\u0432\u043E\u0439
    \u0430\u044D\u0440\u043E\u0434\u0440\u043E\u043C. \u041D\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043D
    \u043E\u0440\u0434\u0435\u043D\u043E\u043C \u041B\u0435\u043D\u0438\u043D\u0430. 
    \u041F\u043E\u0433\u0438\u0431 25.04.1944 \u0433. \u043F\u0440\u0438 \u043E\u0441\u0432\u043E\u0431\u043E\u0436\u0434\u0435\u043D\u0438\u0438
    \u0433\u043E\u0440\u043E\u0434\u0430 \u0421\u0435\u0432\u0430\u0441\u0442\u043E\u043F\u043E\u043B\u044C."
