  _name: "\u041A\u0430\u0440\u0430\u0441\u0451\u0432\u0430"
  _subjectName: "\u041A\u0430\u0440\u0430\u0441\u0451\u0432 \u0418\u0432\u0430\u043D
    \u0420\u043E\u043C\u0430\u043D\u043E\u0432\u0438\u0447"
  _info: "\u0413\u0435\u0440\u043E\u0439 \u0421\u043E\u0432\u0435\u0442\u0441\u043A\u043E\u0433\u043E
    \u0421\u043E\u044E\u0437\u0430 (29.10.1943). \u0420\u043E\u0434\u0438\u043B\u0441\u044F
    02.05.1922 \u0433. \u0432 \u0411\u0435\u043B\u0430\u0440\u0443\u0441\u0438, \u0432
    \u0434\u0435\u0440\u0435\u0432\u043D\u0435 \u0412\u0438\u043B\u0435\u0439\u043A\u0430
    \u0427\u0430\u0443\u0441\u0441\u043A\u043E\u0433\u043E \u0440\u0430\u0439\u043E\u043D\u0430
    \u041C\u043E\u0433\u0438\u043B\u0451\u0432\u0441\u043A\u043E\u0439 \u043E\u0431\u043B\u0430\u0441\u0442\u0438
    \u0432 \u043A\u0440\u0435\u0441\u0442\u044C\u044F\u043D\u0441\u043A\u043E\u0439
    \u0441\u0435\u043C\u044C\u0435. \u041E\u043A\u043E\u043D\u0447\u0438\u043B \u041C\u0443\u0440\u043C\u0430\u043D\u0441\u043A\u043E\u0435
    \u0443\u0447\u0438\u043B\u0438\u0449\u0435 \u0441\u0432\u044F\u0437\u0438 (1946).
    \u0421 1935 \u0433. \u0440\u0430\u0431\u043E\u0442\u0430\u043B \u0432 \u043A\u043E\u043B\u0445\u043E\u0437\u0435
    \u0438 \u043D\u0430 \u0441\u0442\u0440\u043E\u0439\u043A\u0435. \u0412 \u0412\u0435\u043B\u0438\u043A\u0443\u044E
    \u041E\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u0443\u044E
    \u0432\u043E\u0439\u043D\u0443 \u0432\u043E\u0435\u0432\u0430\u043B \u043D\u0430
    \u0421\u0435\u0432\u0435\u0440\u043E-\u0417\u0430\u043F\u0430\u0434\u043D\u043E\u043C,
    \u0421\u0442\u0435\u043F\u043D\u043E\u043C, \u0412\u043E\u0440\u043E\u043D\u0435\u0436\u0441\u043A\u043E\u043C,
    1-\u043C \u0438 2-\u043C \u0423\u043A\u0440\u0430\u0438\u043D\u0441\u043A\u0438\u0445
    \u0444\u0440\u043E\u043D\u0442\u0430\u0445. \u0423\u0447\u0430\u0441\u0442\u043D\u0438\u043A
    \u041A\u0443\u0440\u0441\u043A\u043E\u0439 \u0431\u0438\u0442\u0432\u044B 1943
    \u0433., \u041A\u043E\u0440\u0441\u0443\u043D\u044C-\u0428\u0435\u0432\u0447\u0435\u043D\u043A\u043E\u0432\u0441\u043A\u043E\u0439
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u0438 1944 \u0433., \u043E\u0441\u0432\u043E\u0431\u043E\u0436\u0434\u0435\u043D\u0438\u044F
    \u0423\u043A\u0440\u0430\u0438\u043D\u044B, \u041C\u043E\u043B\u0434\u043E\u0432\u044B,
    \u0420\u0443\u043C\u044B\u043D\u0438\u0438, \u0412\u0435\u043D\u0433\u0440\u0438\u0438. 
    \u041A\u043E\u043C\u0430\u043D\u0434\u0438\u0440 \u043E\u0442\u0434\u0435\u043B\u0435\u043D\u0438\u044F
    \u0441\u0432\u044F\u0437\u0438 1318-\u0433\u043E \u0441\u0442\u0440\u0435\u043B\u043A\u043E\u0432\u043E\u0433\u043E
    \u043F\u043E\u043B\u043A\u0430 (163-\u044F \u0441\u0442\u0440\u0435\u043B\u043A\u043E\u0432\u0430\u044F
    \u0434\u0438\u0432\u0438\u0437\u0438\u044F, 38-\u044F \u0430\u0440\u043C\u0438\u044F,
    \u0412\u043E\u0440\u043E\u043D\u0435\u0436\u0441\u043A\u0438\u0439 \u0444\u0440\u043E\u043D\u0442)
    \u043C\u043B\u0430\u0434\u0448\u0438\u0439 \u0441\u0435\u0440\u0436\u0430\u043D\u0442
    \u0418.\u0420. \u041A\u0430\u0440\u0430\u0441\u0451\u0432 \u043E\u0442\u043B\u0438\u0447\u0438\u043B\u0441\u044F
    \u043F\u0440\u0438 \u0444\u043E\u0440\u0441\u0438\u0440\u043E\u0432\u0430\u043D\u0438\u0438
    \u0414\u043D\u0435\u043F\u0440\u0430 \u044E\u0436\u043D\u0435\u0435 \u041A\u0438\u0435\u0432\u0430.
    02.10.1943 \u0433. \u0432 \u0447\u0438\u0441\u043B\u0435 \u043F\u0435\u0440\u0432\u044B\u0445
    \u043F\u0435\u0440\u0435\u043F\u0440\u0430\u0432\u0438\u043B\u0441\u044F \u043D\u0430
    \u043F\u0440\u0430\u0432\u044B\u0439 \u0431\u0435\u0440\u0435\u0433 \u0440\u0435\u043A\u0438,
    \u043F\u043E\u0434 \u0432\u0440\u0430\u0436\u0435\u0441\u043A\u0438\u043C \u043E\u0433\u043D\u0451\u043C
    \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u043B \u0441\u0432\u044F\u0437\u044C
    \u0434\u0435\u0441\u0430\u043D\u0442\u0430 \u0441 \u043A\u043E\u043C\u0430\u043D\u0434\u043D\u044B\u043C
    \u043F\u0443\u043D\u043A\u0442\u043E\u043C \u043F\u043E\u043B\u043A\u0430, \u0432\u043E
    \u0432\u0440\u0435\u043C\u044F \u0431\u043E\u0451\u0432 \u0437\u0430 2 \u0441\u0443\u0442\u043E\u043A
    \u0438\u0441\u043F\u0440\u0430\u0432\u0438\u043B 27 \u043F\u043E\u0432\u0440\u0435\u0436\u0434\u0435\u043D\u0438\u0439
    \u0442\u0435\u043B\u0435\u0444\u043E\u043D\u043D\u043E\u0433\u043E \u043A\u0430\u0431\u0435\u043B\u044F.
    \u041D\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043D \u043E\u0440\u0434\u0435\u043D\u0430\u043C\u0438
    \u041B\u0435\u043D\u0438\u043D\u0430, \u041A\u0440\u0430\u0441\u043D\u043E\u0439
    \u0417\u0432\u0435\u0437\u0434\u044B, \u043C\u0435\u0434\u0430\u043B\u044F\u043C\u0438.
    \u0421 \u043C\u0430\u0440\u0442\u0430 1953 \u0433. \u0441\u0442\u0430\u0440\u0448\u0438\u0439
    \u043B\u0435\u0439\u0442\u0435\u043D\u0430\u043D\u0442 \u0418.\u0420. \u041A\u0430\u0440\u0430\u0441\u0451\u0432
    \u0432 \u0437\u0430\u043F\u0430\u0441\u0435. \u0416\u0438\u043B \u043D\u0430
    \u0440\u043E\u0434\u0438\u043D\u0435.  \u0423\u043C\u0435\u0440 07.11.1955 \u0433."
